CREATE TABLE CLIENT_MASTER(
CLIENTNO VARCHAR(6),
CLIENTNAME VARCHAR(20),
CITY VARCHAR (15),
PINCODE NUMBER(6),
STATE VARCHAR(15),
BALDUE NUMBER (6)
);
SELECT * FROM CLIENT_MASTER;
